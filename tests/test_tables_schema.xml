<!-- Assignment of selectors to answer values of dynamic answer types -->
<table name="dyna_answer_selectors">
    <field name="avid" type="I">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="selector" type="C" size="255">
